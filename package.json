{
  "name": "comicsscroller",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./ComicsScroller",
    "mkdir": "mkdir -p ./ComicsScroller/js && mkdir -p ./ComicsScroller/css",
    "copy:dev": "cp -r ./src/extensions/* ./ComicsScroller/ && rm -rf ./ComicsScroller/app.html ./ComicsScroller/popup.html && mv ./ComicsScroller/app_dev.html ./ComicsScroller/app.html && mv ./ComicsScroller/popup_dev.html ./ComicsScroller/popup.html",
    "set:dev": "npm run clean && npm run mkdir && npm run copy:dev ",
    "start": "npm run set:dev && npm run compile:background && NODE_ENV=development node server.js",
    "compile:background": "webpack --config webpack.config.bg.js",
    "copy:prod": "cp -r ./src/extensions/* ./ComicsScroller/ && rm -rf ./ComicsScroller/app_dev.html ./ComicsScroller/popup_dev.html",
    "set:prod": "npm run clean && npm run mkdir && npm run copy:prod",
    "product": "npm run set:prod && npm run compile:background && BABEL_ENV=production webpack --config webpack.config.prod.js",
    "test": "jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/src/js/__mocks__/svgMock.jsx",
      "\\.(css)$": "<rootDir>/src/js/__mocks__/cssMock.js",
      "^css(.*)$": "<rootDir>/src/css$1",
      "^imgs(.*)$": "<rootDir>/src/imgs$1",
      "^cmp(.*)$": "<rootDir>/src/js/compoenent$1"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "verbose": true
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-remove-strict-mode": "^0.0.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-babili": "^0.0.12",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.20.0",
    "babili": "^0.0.12",
    "babili-webpack-plugin": "^0.0.11",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.0",
    "empty": "^0.10.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jsdom": "^10.0.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.2",
    "lodash-webpack-plugin": "^0.11.0",
    "normalize.css": "^6.0.0",
    "postcss-loader": "^1.2.1",
    "postcss-nested": "^1.0.0",
    "react": "^15.4.1",
    "react-addons-perf": "^15.4.1",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.1",
    "react-svg-loader": "^1.1.1",
    "react-test-renderer": "^15.5.4",
    "redbox-react": "^1.3.3",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.0",
    "redux-observable": "^0.14.0",
    "reselect": "^3.0.0",
    "rxjs": "^5.2.1-smooth",
    "style-loader": "^0.16.0",
    "webpack": "^2.1.0-beta.28",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.17.0"
  }
}